{
  "name": "open-timetable-scrapper-server",
  "version": "1.0.0",
  "private": true,
  "description": "Open Timetable Scrapper Server",
  "author": {
    "name": "StudentSphere",
    "email": "contact@studentsphere.app",
    "url": "https://studentsphere.app"
  },
  "social": {
    "x": "https://x.com/studentsphere-app",
    "linkedin": "https://linkedin.com/in/studentsphere-app",
    "discord": "https://discord.gg/n4r6QrhvYj"
  },
  "license": "MIT",
  "homepage": "https://github.com/studentsphere-app/ots-server#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/studentsphere-app/ots-server.git"
  },
  "bugs": {
    "url": "https://github.com/studentsphere-app/ots-server/issues"
  },
  "keywords": [
    "ots",
    "studentsphere",
    "timetable",
    "scrapper",
    "scheduler",
    "express",
    "vite",
    "prisma",
    "bullmq",
    "typescript",
    "fullstack"
  ],
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:server\" \"pnpm run dev:client\"",
    "dev:server": "tsx src/index.ts",
    "dev:client": "vite",
    "build": "pnpm run build:server && pnpm run build:client",
    "build:server": "tsup src/index.ts --format esm --clean --minify && cp -r src/templates dist/templates",
    "build:client": "tsc --project tsconfig.client.json --noEmit && vite build",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "clean:queue": "tsx clean-queue.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "@studentsphere/ots-core": "1.0.1",
    "@studentsphere/ots-provider-wigor": "1.0.2",
    "@tailwindcss/vite": "^4.2.1",
    "axios": "^1.13.5",
    "better-auth": "^1.4.19",
    "better-sqlite3": "^12.6.2",
    "bullmq": "^5.70.1",
    "cors": "^2.8.6",
    "dotenv": "^17.3.1",
    "express": "^5.2.1",
    "i18next": "^25.8.13",
    "i18next-browser-languagedetector": "^8.2.1",
    "ioredis": "^5.9.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.575.0",
    "nodemailer": "^8.0.1",
    "prisma": "^7.4.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "react-router-dom": "^7.13.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.3.0",
    "@types/nodemailer": "^7.0.11",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.27",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "@prisma/engines",
      "prisma",
      "esbuild",
      "msgpackr-extract"
    ]
  }
}
